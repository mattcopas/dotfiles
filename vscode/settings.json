{
  "vim.handleKeys": {
    "<C-d>": true,
    "<C-^>": true
  },
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["Down"],
      "after": [""]
    },
    {
      "before": ["Up"],
      "after": [""]
    },
    {
      "before": ["Left"],
      "after": [""]
    },
    {
      "before": ["Right"],
      "after": [""]
    },
    {
      "before": ["<leader>", "<leader>"],
      "commands": [":nohl"]
    }
  ],
  "vim.hlsearch": true,
  "vim.incsearch": true,
  "vim.smartcase": true,
  "vim.leader": "<space>",
}
