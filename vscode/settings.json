{
  "vim.handleKeys": {
      "<C-d>": true,
      "<C-^>": true
    },
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "<leader>"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["<leader>", "g", "g"],
      "commands": [
        "magit.status"
      ]
    },
    {
      "before": ["<leader>", "w"],
      "commands": [":w"]
    }
  ],
  "vim.hlsearch": true,
  "vim.incsearch": true,
  "vim.smartcase": true,
  "vim.leader": "<space>"
}
